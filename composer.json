{
    "name": "dothiv/dothiv-valueobject-bundle",
    "type": "symfony-bundle",
    "description": "dotHIV valueobject bundle for Symfony2 is a collection of value objects.",
    "keywords": ["valueobject", "symfony2", "bundle"],
    "homepage": "http://github.com/dotHIV/DothivValueObjectBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "TLD dotHIV Registry GmbH",
            "homepage": "https://tld.hiv/"
        }
    ],

    "require": {
        "php": ">=5.3.9",
        "symfony/framework-bundle": "~2.3",
        "jms/serializer": "~0.16.0"
    },                                                                                                                                                                     
    "require-dev": {
        "phploc/phploc": "*",
        "pdepend/pdepend": "*",
        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "*",
        "sebastian/phpcpd": "*",
        "covex-nn/phpcb": "*",
        "phpunit/phpunit": "*"
    },

    "config": {
        "bin-dir": "bin"
    },

    "autoload": {
        "psr-0": {
            "Dothiv\\ValueObject": ""
        }
    },

    "target-dir": ""
}
